{
  "name": "storyblok-components",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "cypress:install": "cypress install",
    "cypress:open": "cypress open",
    "cypress:component": "cypress run --component",
    "cypress:run": "cypress run",
    "storyblok": "storyblok",
    "storyblok:login": "storyblok login",
    "storyblok:logout": "storyblok logout",
    "storyblok:pull": "storyblok components pull",
    "storyblok:types": "storyblok types generate --type-prefix Storyblok",
    "storyblok:generate": "sh -c 'pnpm storyblok:pull \"$@\" && pnpm storyblok:types \"$@\"' --",
    "storyblok:proxy": "echo 'Proxy not available for components package - use client app'"
  },
  "dependencies": {
    "@storyblok/react": "^5.4.3",
    "@storyblok/richtext": "^3.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "storyblok-rich-text-react-renderer": "^3.0.1",
    "classnames": "^2.5.1"
  },
  "devDependencies": {
    "@cypress/react": "^9.0.1",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "cypress": "^14.5.3",
    "eslint": "^9",
    "storyblok": "^4.3.2",
    "typescript": "^5",
    "vite": "^6.0.7"
  }
}
