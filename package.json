{
  "name": "ww-marketing-website",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "check-types": "turbo run check-types",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,json,css,scss}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,md,json,css,scss}\"",
    "setup:env": "turbo run setup:env",
    "cypress:install": "turbo run cypress:install",
    "cypress:open": "turbo run cypress:open",
    "cypress:component": "turbo run cypress:component",
    "cypress:run": "turbo run cypress:run",
    "certificate:install": "turbo run certificate:install",
    "openssl": "turbo run openssl",
    "openssl:generate-ssls": "turbo run openssl:generate-ssls",
    "storyblok": "pnpm --filter=client storyblok",
    "storyblok:login": "pnpm --filter=client storyblok:login",
    "storyblok:logout": "pnpm --filter=client storyblok:logout",
    "storyblok:pull": "pnpm --filter=client storyblok:pull",
    "storyblok:types": "pnpm --filter=client storyblok:types",
    "storyblok:generate": "pnpm --filter=client storyblok:generate",
    "storyblok:proxy": "pnpm --filter=client storyblok:proxy",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "changeset:status": "changeset status",
    "version-packages": "changeset version && pnpm install --lockfile-only",
    "prepare": "husky"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@eslint/eslintrc": "^3",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.8.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,json,css,scss}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
