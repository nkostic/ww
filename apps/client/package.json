{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "build": "next build",
    "start": "next start --port 3000",
    "lint": "next lint",
    "check-types": "tsc --noEmit",
    "setup:env": "cp .env.development.example .env.development && echo 'âœ… Created .env.development - Please edit with your Storyblok credentials'",
    "cypress:install": "cypress install",
    "cypress:open": "cypress open",
    "cypress:component": "cypress run --component",
    "cypress:run": "cypress run",
    "storyblok": "storyblok",
    "storyblok:login": "storyblok login",
    "storyblok:logout": "storyblok logout",
    "storyblok:pull": "storyblok components pull -p src/types/storyblok --sf --space=$SPACE_ID",
    "storyblok:pull-with-space": "storyblok components pull -p src/types/storyblok --sf --space=",
    "storyblok:types": "storyblok types generate -p src/types/storyblok --type-prefix=Storyblok --sf --space=$SPACE_ID",
    "storyblok:generate": "pnpm storyblok:pull && pnpm storyblok:types",
    "storyblok:proxy": "local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "certificate:install": "mkcert localhost && mkcert -install",
    "openssl": "openssl",
    "openssl:generate-ssls": "openssl req -x509 -newkey rsa:2048 -keyout localhost-key.pem -out localhost.pem -days 365 -nodes"
  },
  "dependencies": {
    "@repo/storyblok-components": "workspace:*",
    "@storyblok/react": "^5.4.3",
    "@storyblok/richtext": "^3.6.0",
    "classnames": "^2.5.1",
    "cookie": "^1.0.2",
    "cypress": "^14.5.3",
    "he": "^1.2.0",
    "html-entities": "^2.6.0",
    "jest": "^30.0.5",
    "local-ssl-proxy": "^2.0.5",
    "next": "15.4.5",
    "react": "19.1.0",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "19.1.0",
    "storyblok-rich-text-react-renderer": "^3.0.1"
  },
  "devDependencies": {
    "@cypress/react": "^9.0.1",
    "@cypress/webpack-dev-server": "^4.1.1",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "storyblok": "^4.3.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "packageManager": "pnpm@9.0.0"
}
